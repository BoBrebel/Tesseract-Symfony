<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Tesseract\UserBundle\Entity\InscriptionEvenement" table="inscription_evenement">
    <indexes>
      <index name="id_utilisateur" columns="id_utilisateur"/>
      <index name="id_evenement" columns="id_evenement"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="date" column="date" nullable="true"/>
    <many-to-one field="idUtilisateur" target-entity="Utilisateur">
      <join-columns>
        <join-column name="id_utilisateur" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEvenement" target-entity="Evenement">
      <join-columns>
        <join-column name="id_evenement" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
