




<div class="row">
    <div class="col-md-4">
        <div class="card">
            <div class="card-body no-padding">
                <ul class="list divider-full-bleed">
                    {% for entity in entities %}
                        <li class="tile">
                            <a class="tile-content ink-reaction" href="#2" style="width: 80%">
                                <div class="tile-icon">
                                    <img src="{{ asset(entity.getIdUtilisateur().getPhoto())}}" alt="" />
                                </div>
                                <div class="tile-text">{{entity.getIdUtilisateur().getUsername()}} {{entity.tache}} {{entity.cible}}</div>
                            </a>
                            <a class="ink-reaction">
                                <h4>{{date()|date("d")- entity.getDate()|date("d")}} Days ago</h4>
                            </a>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>






