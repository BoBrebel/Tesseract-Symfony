{% extends "TesseractMOOCBundle:Admin:Admindashboard.html.twig" %}

{% block content %}
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <div class="card">
            <div class="card-body no-padding">
                <ul class="list divider-full-bleed">

                        <li class="tile">
                            <a class="tile-content ink-reaction" href="#2" style="alignment: center">
                                <div class="tile-icon">
                                    <img src="{{ asset(user.getPhoto())}}" alt="" />
                                </div>
                                <div class="tile-text">{{user.getUsername()}} Has Been Banned  for a week</div>
                            </a>

                        </li>

                </ul>
            </div>
        </div>
    </div>

{% endblock %}