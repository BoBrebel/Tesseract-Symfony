<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>  
<script src="//code.highcharts.com/4.0.1/highcharts.js"></script> 
<script src="//code.highcharts.com/4.0.1/modules/exporting.js"></script> 
<script type="text/javascript">
    {{ chart(chart) }} 
</script>  
<div class="container-fluid">
<div class="row">
    
        <div class="card">
            <div class="row">
                <div class="col-md-8">
                    <div class="card-head">
                        <header>Site activity</header>
                    </div>
                    <div class="card-body height-8">
                        <div id="linechart" class="flot height-7"></div> 
                    </div>

                </div>
                <div class="col-md-4">
                    <div class="card-head">
                        <header>Today's stats</header>
                    </div>
                    <div class="card-body height-8">
                        <strong>{{nbrcoaches}}</strong> Coaches
                        <span class="pull-right text-success text-sm">{{(nbrcoaches/nbr)*100}}% <i class="md md-trending-up"></i></span>
                        <div class="progress progress-hairline">
                            <div class="progress-bar progress-bar-primary-dark" style="width:10%"></div>
                        </div>
                        <strong>{{nbrstudents}}</strong> Students
                        <span class="pull-right text-danger text-sm">{{(nbrstudents/nbr)*100}}%<i class="md md-trending-down"></i></span>
                        <div class="progress progress-hairline">
                            <div class="progress-bar progress-bar-danger" style="width:93%"></div>
                        </div>
                        <strong>{{nbr}}</strong> Users
                        <span class="pull-right text-success text-sm">0,18% <i class="md md-trending-up"></i></span>
                        <div class="progress progress-hairline">
                            <div class="progress-bar progress-bar-primary-dark" style="width:47%"></div>
                        </div>
                    </div>
                </div>
            </div>
        
    </div>
</div>
</div>
